{
	"name": "faithful-api",
	"version": "2.0.0",
	"description": "Public RESTful API for Faithful's texture database, add-on submission, and more.",
	"scripts": {
		"dev": "pnpm run spec-and-routes && pnpm run nodemon",
		"nodemon": "nodemon",
		"compile": "tsc",
		"spec-and-routes": "tsoa spec-and-routes",
		"exec": "ts-node src/index.ts",
		"build": "pnpm run spec-and-routes && pnpm run compile && cp -R public dist/",
		"start": "node dist/src/index.js",
		"swag": "tsoa spec",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"prettier": "prettier \"{,!(node_modules)/**/}*.{js,ts}\" --config .prettierrc --write"
	},
	"keywords": [],
	"author": "",
	"devDependencies": {
		"@types/api-error-handler": "^1.0.36",
		"@types/body-parser": "^1.19.5",
		"@types/cors": "^2.8.18",
		"@types/express": "^4.17.22",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.15.21",
		"@types/response-time": "^2.3.8",
		"@types/statuses": "^2.0.5",
		"@types/swagger-ui-express": "^4.1.8",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.2.6",
		"nodemon": "^3.1.10",
		"prettier": "^3.5.3",
		"ts-node": "^10.9.2"
	},
	"dependencies": {
		"@tsoa/runtime": "^6.6.0",
		"api-error-handler": "^1.0.0",
		"axios": "^1.9.0",
		"body-parser": "^2.2.0",
		"cloudflare": "^4.3.0",
		"cors": "^2.8.5",
		"discord-api-types": "^0.38.8",
		"dotenv": "^16.5.0",
		"express": "^4.21.2",
		"firestorm-db": "^1.13.0",
		"form-data": "^4.0.2",
		"isomorphic-dompurify": "^2.25.0",
		"multer": "^2.0.0",
		"response-time": "^2.3.3",
		"statuses": "^2.0.1",
		"swagger-ui-express": "^5.0.1",
		"tsoa": "^6.6.0"
	}
}
